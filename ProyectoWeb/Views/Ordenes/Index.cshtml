
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">

        <table id="mytable" class="table table-striped table-bordered table-responsive">
            <h1 class="title-top-table">
                Cliente
            <tr><th>Lunes</th></tr>
                @foreach (var lunes in (List<String>)(ViewBag.lunes))
                {
                    <tr><td>@lunes</td></tr>
                }
        </table>
    </div>
</div>

@section scripts
{
    <script>
                $("li.ordenes").addClass("active");
    </script>
}




